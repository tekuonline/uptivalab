{
  "name": "uptivalab-api",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --dts --format esm --out-dir dist && tsup src/routes/users.ts src/routes/invitations.ts src/routes/recorder.ts --format esm --out-dir dist --no-dts --no-clean",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@fastify/compress": "6.5.0",
    "@fastify/cors": "9.0.1",
    "@fastify/jwt": "8.0.0",
    "@fastify/rate-limit": "9.0.0",
    "@fastify/swagger": "8.12.0",
    "@fastify/swagger-ui": "3.0.0",
    "@fastify/websocket": "10.0.1",
    "@grpc/grpc-js": "1.9.13",
    "@grpc/proto-loader": "0.7.10",
    "@playwright/test": "1.58.0",
    "@prisma/client": "5.15.0",
    "@uptivalab/monitoring": "workspace:*",
    "@uptivalab/shared": "workspace:*",
    "argon2": "0.40.1",
    "bullmq": "5.10.3",
    "dockerode": "4.0.8",
    "dotenv": "16.4.5",
    "execa": "8.0.1",
    "fastify": "4.29.1",
    "fastify-plugin": "4.5.1",
    "ioredis": "5.4.1",
    "jsonwebtoken": "9.0.2",
    "mongodb": "6.6.1",
    "mysql2": "3.9.8",
    "nodemailer": "7.0.11",
    "pg": "8.11.5",
    "pino": "9.1.0",
    "playwright-core": "1.57.0",
    "prisma": "5.15.0",
    "redis": "4.6.13",
    "tslib": "2.6.2",
    "undici": "6.13.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.12.7",
    "@types/nodemailer": "^7.0.4",
    "@types/supertest": "2.0.16",
    "eslint": "9.0.0",
    "supertest": "7.0.0",
    "tsup": "8.0.2",
    "tsx": "4.7.2",
    "typescript": "5.4.5",
    "vitest": "1.6.1"
  }
}
